{
  "name": "node-boilerplate",
  "version": "0.0.0",
  "description": "A boilerplate for simple Node projects",
  "main": "./src/index.js",
  "scripts": {
    "clean": "rm -rf dist || 0",
    "build": "npm run clean && npx babel -d ./dist ./src -s",
    "start": "DEBUG=app* nodemon --exec npx babel-node ./src/index.js",
    "test": "jest"
  },
  "author": "Brodey Newman",
  "license": "MIT",
  "dependencies": {
    "body-parser": "^1.18.3",
    "cors": "^2.8.5",
    "debug": "^4.1.1",
    "dotenv": "^6.2.0",
    "express": "^4.16.4",
    "lodash": "^4.17.11",
    "node-fetch": "^2.3.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.3.4",
    "@babel/node": "^7.2.2",
    "@babel/preset-env": "^7.3.4",
    "babel-eslint": "^10.0.1",
    "babel-plugin-lodash": "^3.3.4",
    "eslint": "^5.12.1",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-jsx-a11y": "^6.2.0",
    "eslint-plugin-react": "^7.12.4",
    "nodemon": "^1.18.10"
  }
}
